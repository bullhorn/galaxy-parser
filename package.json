{
  "name": "galaxy-parser",
  "version": "1.0.45",
  "description": "Analyzes your code metric reports and will parse the results into Galaxy",
  "preferGlobal": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel src --out-dir lib --copy-files",
    "compile:watch": "npm run compile -- --watch",
    "prepublish": "npm run compile"
  },
  "bin": {
    "galaxy": "./lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgodi/galaxy-parser"
  },
  "keywords": [
    "galaxy",
    "static",
    "code",
    "analysis",
    "firebase"
  ],
  "author": "Joshua Godi <joshuagodi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgodi/galaxy-parser/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/jgodi/galaxy-parser#README",
  "dependencies": {
    "async": "^2.0.1",
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "commander": "^2.9.0",
    "easy-table": "^1.0.0",
    "github": "^9.2.0",
    "lcov-parse": "0.0.10",
    "node-gitlab": "^1.6.0",
    "request": "^2.74.0",
    "simple-git": "^1.48.0",
    "sloc": "^0.1.10",
    "xml2js": "^0.4.17",
    "yargs": "^5.0.0"
  }
}